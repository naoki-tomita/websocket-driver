{
  "name": "websocket-webdriver",
  "version": "1.0.9",
  "main": "dist/src/scripts/server/index.js",
  "types": "dist/src/scripts/server/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@types/socket.io": "1.4.33",
    "@types/socket.io-client": "1.4.32",
    "socket.io": "2.1.0"
  },
  "devDependencies": {
    "@types/html2canvas": "^0.0.33",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/socket.io": "1.4.33",
    "@types/socket.io-client": "1.4.32",
    "@types/webpack": "4.1.4",
    "@types/ws": "4.0.2",
    "awesome-typescript-loader": "5.0.0",
    "babel-polyfill": "^6.26.0",
    "chokidar-cli": "1.2.0",
    "html2canvas": "^1.0.0-alpha.12",
    "jest": "^22.4.3",
    "local-web-server": "2.5.1",
    "nodejs-scanf": "^1.1.0",
    "npm-run-all": "4.1.2",
    "prettier": "^1.12.1",
    "ts-jest": "^22.4.6",
    "ts-node": "6.0.1",
    "typescript": "2.8.3",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15"
  },
  "scripts": {
    "cli": "run-p serve:webserver cli:server",
    "cli:server": "ts-node ./cli.ts",
    "clean": "rm -r -f ./dist",
    "prebuild": "run-p clean",
    "build": "run-p build:*",
    "build:client": "run-p build:client:*",
    "build:client:ts": "webpack --config ./webpack.config.ts",
    "build:server": "run-p build:server:*",
    "build:server:ts": "tsc",
    "serve": "run-p serve:*",
    "serve:websocket": "ts-node ./test2.ts",
    "serve:webserver": "ws -p 8080 --https",
    "watch": "run-p watch:*",
    "watch:client": "run-p watch:client:*",
    "watch:client:ts": "chokidar \"./src/scripts/client/**/*\" -c \"yarn build:client:ts\"",
    "prestart": "run-p build",
    "start": "run-p watch serve",
    "test": "jest --config ./jest.config.json",
    "prepublish": "run-p build"
  }
}
